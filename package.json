{
    "name": "eigen-explorer-backend",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "tsc",
        "start": "tsx watch --clear-screen=false ./src/index.ts",
        "format": "npx @biomejs/biome format . --write",
        "prisma:generate": "prisma generate --schema=./src/prisma/schema.prisma",
        "prisma:seed": "tsx ./src/prisma/seed.ts",
        "prisma:format": "prisma format --schema=./src/prisma/schema.prisma",
        "generate-docs": "tsx ./docs/generateSwagger.ts"
    },
    "dependencies": {
        "@prisma/client": "^5.11.0",
        "cookie-parser": "~1.4.4",
        "debug": "~2.6.9",
        "dotenv": "^16.4.5",
        "express": "^4.18.3",
        "helmet": "^7.1.0",
        "http-errors": "~1.6.3",
        "jade": "^0.29.0",
        "joi": "^17.12.2",
        "morgan": "~1.9.1",
        "viem": "^2.8.14"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.6.3",
        "@types/cookie-parser": "^1.4.7",
        "@types/debug": "^4.1.12",
        "@types/express": "^4.17.21",
        "@types/morgan": "^1.9.9",
        "@types/node": "^20.12.2",
        "prisma": "^5.11.0",
        "swagger-jsdoc": "^6.2.8",
        "tsx": "^4.7.1",
        "typescript": "^5.4.3"
    }
}
