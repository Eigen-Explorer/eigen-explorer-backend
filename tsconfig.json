{
	"compilerOptions": {
		"target": "ES6",
		"module": "ES6",
		"esModuleInterop": true,
		"moduleResolution": "node",
		"sourceMap": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		
		"outDir": "dist",
		"baseUrl": "./src"
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}
